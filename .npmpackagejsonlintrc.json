{
    "$schema": "https://json.schemastore.org/npmpackagejsonlintrc",
    "extends": "npm-package-json-lint-config-default",
    "overrides": [
        {
            "patterns": [
                "./package.json"
            ],
            "rules": {
                "require-version": "off"
            }
        }
    ],
    "rules": {
        "require-repository": "error"
    }
}
